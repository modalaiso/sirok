{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "scrollbar-color",
          "scrollbar-width",
          "scrollbar-width: thin"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "disown-opener": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 137",
    "not firefox <= 137",
    "not ios_saf <= 18.4",
    "not safari <= 18.4"
  ]
}